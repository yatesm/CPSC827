				(x, FORMALVALUEPARAMETER, 1, -)
				(-, ENDFORMALPARAMETERS, -, -)
				(-, CALL, PRINTF, -)
				(-, BEGINOUTPUTPARAMETER,  "x = %d\n", -)
				(-, OUTPUTPARAMETER, x, 1)
				(-, ENDOUTPUTPARAMETER, -, -)
